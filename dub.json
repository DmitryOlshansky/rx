{
	"name": "rx",
	"description": "Reactive Extensions for D Programming Language.",
	"copyright": "Copyright Â© 2015, lempiji",
	"license": "MIT",
	"homepage": "https://github.com/lempiji/rx",
	"authors": [
		"lempiji"
	],
	"sourcePaths": [
		"source"
	],
	"configurations": [
		{
			"name": "default",
			"targetType": "library"
		},
		{
			"name": "ddox",
			"buildOptions": ["syntaxOnly"],
			"dflags": ["-c", "-Df__dummy.html", "-Xfdocs.json"],
			"postBuildCommands-windows": [
				"dub fetch ddox",
				"dub run ddox --arch=x86_mscoff -- filter docs.json --only-documented",
				"dub run ddox --arch=x86_mscoff -- generate-html docs.json docs",
				"del __dummy.html"
			]
		}
	]
}